{
  "name": "illinois-cs-wiki",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format:check": "prettier . --check || eslint .",
    "format:fix": "prettier . --write || eslint . --fix",
    "postinstall": "npm run setup && husky install",
    "prepare": "husky || true",
    "setup": "git config blame.ignoreRevsFile .git-blame-ignore-revs"
  },
  "lint-staged": {
    "*.{ts,tsx,astro,js,css,md,mdx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{yaml,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/sitemap": "^3.7.0",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.17.2",
    "dayjs": "^1.11.19",
    "astro-compress": "^2.3.9",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/parser": "^8.54.0",
    "astro": "^5.17.1",
    "astro-eslint-parser": "^1.2.2",
    "astro-icon": "^1.1.5",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}